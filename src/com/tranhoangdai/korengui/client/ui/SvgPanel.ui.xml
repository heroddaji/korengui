<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:d="urn:import:com.tranhoangdai.korengui.client.ui">
	<ui:with type="com.tranhoangdai.korengui.resources.KorenResources" field="res"></ui:with>
	<g:HTMLPanel styleName="{res.css.daiBorder}">
		<b:TabPanel tabPosition="above" ui:field="panel">
			<b:TabPane heading="Global" active="true" ui:field="firstTabPane">
				<d:DrawingPane ui:field="globalDrawingPane" />
			</b:TabPane>
		</b:TabPanel>
	</g:HTMLPanel>
</ui:UiBinder>